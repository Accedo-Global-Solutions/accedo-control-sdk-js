{
  "name": "appgrid",
  "version": "1.0.0",
  "description": "The Official AppGrid SDK for NodeJS",
  "main": "dist/bundle.umd.js",
  "jsnext:main": "dist/bundle.es6.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "precompile": "npm run clean",
    "compile": "npm run compileModule && npm run compileExample",
    "precompileModule": "npm run lint",
    "compileModule": "rollup -c rollup.config.umd.js && rollup -c rollup.config.es6.js",
    "precompileExample": "npm run lintExample",
    "compileExample": "rollup -c rollup.config.example.js",
    "prerunExample": "npm run compile && npm run test",
    "runExample": "node build/example",
    "lint": "eslint . --ext .js",
    "lintExample": "eslint ./example-es6.js --no-ignore",
    "lintTest": "eslint ./test --no-ignore",
    "prepublish": "npm run compile",
    "pretest": "npm run lintTest && rollup -c rollup.config.test.js",
    "test": "mocha --opts ./test/mocha.opts build/test-bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org/accedo/appgrid-sdk-web.git"
  },
  "keywords": [
    "appgrid",
    "accedo"
  ],
  "author": "Accedo, AB",
  "license": "Apache-2.0",
  "homepage": "http://appgrid.accedo.tv/",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-react": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "rollup": "^0.25.6",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-multi-entry": "^1.2.0",
    "source-map-support": "^0.4.0"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.1.0",
    "uuid": "^2.0.1"
  }
}
