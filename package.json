{
  "name": "appgrid-browser",
  "version": "0.1.0",
  "description": "The Official vanilla-JS AppGrid SDK for browsers",
  "main": "dist/bundle.umd.js",
  "browser": "dist/bundle.browser.js",
  "module": "dist/bundle.es6.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "npm run buildLib && npm run buildExample",
    "prebuildLib": "npm run lint",
    "buildLib": "rollup -c rollup.config.umd.js && rollup -c rollup.config.es6.js && rollup -c rollup.config.browser.js",
    "buildExample": "rollup -c rollup.config.example.js",
    "preexample": "npm run build",
    "example": "node build/example",
    "lint": "eslint . --ext .js",
    "pretest": "rollup -c rollup.config.test.js",
    "test": "mocha --opts ./test/mocha.opts build/test-bundle.js",
    "doc": "documentation build src/index.js -c doc_config.yml --format html -o docs",
    "docmd": "documentation build src/index.js -c doc_config.yml --format md > API.md",
    "prepublish": "npm test && npm run doc && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Accedo-Products/appgrid-sdk-node.git"
  },
  "keywords": [
    "appgrid",
    "accedo",
    "sdk",
    "es6"
  ],
  "author": "Accedo, AB",
  "contributors": [
    "Jason Dujardin-Terry",
    "Gregory Desfour",
    "Nicolas Nannoni"
  ],
  "license": "Apache-2.0",
  "homepage": "http://appgrid.accedo.tv/",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "documentation": "~4.0.0-beta11",
    "eslint": "~3.8.1",
    "eslint-config-airbnb-base": "~9.0.0",
    "eslint-import-resolver-node": "^0.2.2",
    "eslint-plugin-import": "~2.0.1",
    "mocha": "~3.1.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sinon": "~1.17.6",
    "source-map-support": "~0.4.5"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qs": "~6.3.0",
    "stampit": "^2.1.2",
    "uuid": "~2.0.3"
  }
}
