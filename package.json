{
  "name": "appgrid",
  "version": "3.0.4",
  "description": "The Official vanilla-JS AppGrid SDK for Node.js and browsers",
  "main": "dist/appgrid.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "npm run buildLib && npm run buildExample",
    "prebuildLib": "npm run lint",
    "buildLib": "rollup -c rollup.config.node.js && rollup -c rollup.config.browser.js",
    "buildExample": "rollup -c rollup.config.example.js",
    "preexample": "npm run build",
    "example": "node build/example",
    "lint": "eslint src/ --ext .js",
    "pretest": "rollup -c rollup.config.test.js",
    "test": "mocha --opts ./test/mocha.opts build/test-bundle.js",
    "doc": "documentation build src/index.js -c doc_config.yml --format html -o docs",
    "docmd": "documentation build src/index.js -c doc_config.yml --format md > API.md",
    "prepublish": "npm test && npm run doc && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Accedo-Products/appgrid-sdk-js.git"
  },
  "keywords": [
    "appgrid",
    "accedo",
    "sdk",
    "es6"
  ],
  "author": "Accedo, AB",
  "contributors": [
    "Jason Dujardin-Terry",
    "Gregory Desfour",
    "Nicolas Nannoni"
  ],
  "license": "Apache-2.0",
  "homepage": "http://appgrid.accedo.tv/",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "documentation": "4.0.0-beta.18",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sinon": "^1.17.6",
    "source-map-support": "^0.4.10"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.3.0",
    "stampit": "^2.1.2",
    "uuid": "^3.0.1"
  }
}
