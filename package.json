{
  "name": "appgrid",
  "version": "2.0.0",
  "description": "The Official AppGrid SDK for Node.js",
  "main": "dist/bundle.umd.js",
  "jsnext:main": "dist/bundle.es6.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "npm run buildLib && npm run buildExample",
    "prebuildLib": "npm run lint",
    "buildLib": "rollup -c rollup.config.umd.js && rollup -c rollup.config.es6.js",
    "buildExample": "rollup -c rollup.config.example.js",
    "preexample": "npm run build",
    "example": "node build/example",
    "lint": "eslint . --ext .js",
    "pretest": "rollup -c rollup.config.test.js",
    "test": "mocha --opts ./test/mocha.opts build/test-bundle.js",
    "doc": "documentation build src/index.js -c doc_config.yml --format html -o docs",
    "docmd": "documentation build src/index.js -c doc_config.yml --format md > API.md",
    "prepublish": "npm test && npm run doc && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Accedo-Products/appgrid-sdk-node.git"
  },
  "keywords": [
    "appgrid",
    "accedo",
    "sdk",
    "node"
  ],
  "author": "Accedo, AB",
  "license": "Apache-2.0",
  "homepage": "http://appgrid.accedo.tv/",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-helper-bindify-decorators": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-stage-2": "^6.11.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "documentation": "^4.0.0-beta9",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-import-resolver-node": "^0.2.2",
    "eslint-plugin-import": "^1.13.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.34.8",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "source-map-support": "^0.4.2"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.2.1",
    "stampit": "^2.1.2",
    "uuid": "^2.0.2"
  }
}
