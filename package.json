{
  "name": "appgrid",
  "version": "3.2.4",
  "description": "The Official vanilla-JS Accedo One SDK for Node.js and browsers (previously known as AppGrid)",
  "main": "src/node/index.js",
  "browser": "dist/appgrid.umd.min.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean && npm run lint",
    "build": "rollup -c rollup.config.browser.js",
    "lint": "eslint {src/,test/} --ext .js",
    "lint:check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "prettier": "prettier --write \"{src,test}/**/*.js\"",
    "test": "jest",
    "doc": "documentation build src/**/index.js -c doc_config.yml --format html -o docs",
    "docmd": "documentation build src/**/index.js -c doc_config.yml --format md > API.md",
    "prepublishOnly": "npm test && npm run doc && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Accedo-Products/appgrid-sdk-js.git"
  },
  "keywords": [
    "AppGrid",
    "Accedo",
    "Accedo One",
    "sdk",
    "es6",
    "javascript"
  ],
  "author": "Accedo, AB",
  "contributors": [
    "Jason Dujardin-Terry",
    "Gregory Desfour",
    "Nicolas Nannoni",
    "Oskar Nordgren"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 4"
  },
  "homepage": "https://www.accedo.tv/one",
  "files": [
    "dist/",
    "src/"
  ],
  "devDependencies": {
    "chalk": "^1.1.3",
    "documentation": "^4.0.0-rc.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jest": "^21.2.1",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.47.6",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "source-map-support": "^0.4.18"
  },
  "dependencies": {
    "isomorphic-unfetch": "^1.0.0",
    "qs": "^6.5.1",
    "stampit": "^2.1.2",
    "uuid": "^3.1.0"
  }
}
