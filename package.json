{
  "name": "appgrid",
  "version": "0.1.0",
  "description": "The Official AppGrid SDK for NodeJS",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "precompile": "npm run clean",
    "compile": "npm run compileModule && npm run compileExample",
    "precompileModule": "npm run lint",
    "compileModule": "babel -d dist/ src/",
    "precompileExample": "npm run lintExample",
    "compileExample": "babel example-es6.js -o example.js",
    "prerunExample": "npm run compile",
    "runExample": "node example",
    "lint": "eslint . --ext .js",
    "lintExample": "eslint ./example-es6.js --no-ignore",
    "prepublish": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://jasondt@bitbucket.org/accedo/appgrid-sdk-web.git"
  },
  "keywords": [
    "appgrid",
    "accedo"
  ],
  "author": "Accedo, AB",
  "license": "Apache-2.0",
  "homepage": "http://appgrid.accedo.tv/",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chalk": "^1.1.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-react": "^4.2.0",
    "estraverse-fb": "^1.3.1"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.1.0",
    "uuid": "^2.0.1"
  }
}
